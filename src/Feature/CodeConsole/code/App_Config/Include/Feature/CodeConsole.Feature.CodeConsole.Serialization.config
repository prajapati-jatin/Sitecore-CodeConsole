<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Feature.CodeConsole" description="Code console feature" dependencies="Foundation.Serialization">
          <targetDataStore physicalRootPath="$(sourceFolder)\feature\codeconsole\serialization" type="Rainbow.Storage.SerializationFileSystemDataStore, Rainbow" useDataCache="false" singleInstance="true" />
          <predicate type="Unicorn.Predicates.SerializationPresetPredicate, Unicorn" singleInstance="true">
            <include name="Feature.Application.CodeConsole" database="core" path="/sitecore/content/Applications/Code Console" />
            <include name="Feature.DocumentsAndSettings.Left" database="core" path="/sitecore/content/Documents and settings/All users/Start menu/Left/Code console" />
            <include name="Feature.Layouts" database="core" path="/sitecore/layout/Layouts/Applications/Foundation/Code Console" />
          </predicate>
        </configuration>
      </configurations>
    </unicorn>
  </sitecore>
</configuration>
